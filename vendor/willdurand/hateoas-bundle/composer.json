{
    "name": "willdurand/hateoas-bundle",
    "description": "Integration of Hateoas into Symfony2.",
    "keywords": [ "rest", "hateoas" ],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "William Durand",
            "email": "william.durand1@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "jms/serializer-bundle": "^3.1 || ^4.0 || ^5.0@beta",
        "symfony/expression-language": "~3.0 || ~4.0 || ~5.0 || ~6.0",
        "symfony/framework-bundle": "~3.0 || ~4.0 || ~5.0 || ~6.0",
        "willdurand/hateoas": "^3.5"
    },
    "require-dev": {
        "doctrine/annotations": "^1.13.2",
        "doctrine/coding-standard": "^8.0",
        "phpunit/phpunit": "^8.5 || ^9.5",
        "symfony/stopwatch": "~3.0 || ~4.0 || ~5.0 || ~6.0",
        "twig/twig": "~1.12|~2.0"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": { "Bazinga\\Bundle\\HateoasBundle\\": "" }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
